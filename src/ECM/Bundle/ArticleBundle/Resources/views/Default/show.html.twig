{% extends 'ECMHomeBundle::layout.html.twig' %}




    {% block titre %}
        <h1>{{ menu | upper }}</h1>

    {% endblock %}


    {% block body %}
        {% for article in articles %}

            <div class="row">
            <div class="col-lg-12">
                <div>{{ article.titre }}</div>
                <p>
                    {{ article.corps | raw }}
                </p>
            </div>
        </div>
        {% endfor %}
    {% endblock %}